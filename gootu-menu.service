[Unit]
Description=Gootu Menu Scraper
After=network.target

[Service]
Type=simple
User=loic
Environment=NODE_ENV=production
WorkingDirectory=/home/loic/projets/gootu-scrapper
ExecStart=/home/loic/projets/gootu-scrapper/.devenv/profile/bin/node main.ts
Restart=always
RestartSec=10

[Install]
WantedBy=multi-user.target